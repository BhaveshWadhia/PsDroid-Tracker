<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.psdroid">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PsDroidNew"
        tools:ignore="ExtraText">
        <activity android:name="com.example.psdroid.ui.login.LoginActivity"
            android:label="LoginActivity" />
        <activity android:name="com.example.psdroid.ui.login.IntroductoryActivity"
            android:label="IntroductoryActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.psdroid.ui.login.ForgotPassword"
            android:label="ForgotPasswordActivity" />
        <activity android:name="com.example.psdroid.ui.settings.PrivacyPolicyActivity"
            android:label="PrivacyPolicy" />
        <activity android:name="com.example.psdroid.ui.settings.FeedbackActivity"
            android:label="Feedback" />
        <activity android:name="com.example.psdroid.ui.settings.TermsConditionsActivity"
            android:label="TermsConditions" />
        <activity android:name="com.example.psdroid.ui.settings.SettingsActivity"
            android:label="Settings" />
        <activity
            android:name="com.example.psdroid.ui.add_users.AddUsersActivity"
            android:label="AddUserActivity" />
        <activity android:name="com.example.psdroid.MainScreen"
            android:label="MainScreenActivity" >
        </activity>
        <activity
            android:name="com.example.psdroid.MainActivity"
            android:label="@string/app_name">
        </activity>
            <meta-data
                android:name="com.google.android.gms.version"
                android:value="@integer/google_play_services_version" />
            <meta-data
                android:name="com.google.android.geo.API_KEY"
                android:value="@string/MAP_KEY" />
    </application>
</manifest>